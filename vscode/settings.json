{
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontSize": 16,
  "editor.fontFamily": "source code variable",
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "editor.renderWhitespace": "all",
  "window.nativeTabs": true,
  "window.restoreWindows": "all",
  "window.openFoldersInNewWindow": "on",
  "files.autoGuessEncoding": false,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.fontFamily": "monaco",
  "git.confirmSync": false,
  "files.associations": {
      "*.mcss": "less"
  },
  "window.zoomLevel": 0,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "clock.dateFormat": "yyyy-mm-dd HH:MM:ss",
  "workbench.statusBar.feedback.visible": false,
  "git.enableSmartCommit": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#0A0A0D",
    "statusBar.debuggingBackground": "#511f1f",
    "scrollbarSlider.activeBackground": "#80CBC450",
    "progressBar.background": "#80CBC4",
    "tab.activeBorder": "#80CBC4",
    "editorWidget.resizeBorder": "#80CBC4",
    "panelTitle.activeBorder": "#80CBC4"
  },
  "editor.detectIndentation": false,
  "editor.snippetSuggestions": "top",
  "gitlens.currentLine.enabled": false,
  "fileHeaderComment.template": {
      "js":[
          "${commentbegin}",
          "${commentprefix} Created on ${date}",
          "${commentprefix} @author ${author}",
          "${commentprefix} @description ${description}",
          "${commentend}"
      ],
      "html":[
          "<!--",
          " Created on ${date}",
          " @author ${author}",
          " @description ${description}",
          "-->"
      ]
  },
  "fileHeaderComment.parameter": {
      "*": {
          "author": "yubaoquan",
          "description": "---"
      }
  },
  "gitlens.blame.format": "${author|15} ${id|10-}",
  "gitlens.statusBar.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "gitlens.keymap": "chorded",
  "gitlens.advanced.messages": {
      "suppressCommitHasNoPreviousCommitWarning": false,
      "suppressCommitNotFoundWarning": false,
      "suppressFileNotUnderSourceControlWarning": false,
      "suppressGitVersionWarning": false,
      "suppressLineUncommittedWarning": false,
      "suppressNoRepositoryWarning": false,
      "suppressResultsExplorerNotice": false,
      "suppressShowKeyBindingsNotice": true
  },
  "sync.gist": "922a644a9a258f14de6cdde1d37cd0e3",
  "sync.autoDownload": false,
  "sync.autoUpload": false,
  "sync.forceDownload": false,
  "sync.quietSync": false,
  "sync.askGistName": false,
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  "editor.wordWrap": "on",
  "gitlens.menus": {
      "editor": {
          "blame": true,
          "clipboard": false,
          "compare": false,
          "details": false,
          "history": false,
          "remote": false
      },
      "editorGroup": {
          "blame": true,
          "compare": false,
          "history": false,
          "remote": false
      },
      "editorTab": {
          "compare": false,
          "history": false,
          "remote": false
      },
      "explorer": {
          "compare": true,
          "history": true,
          "remote": false
      }
  },
}
